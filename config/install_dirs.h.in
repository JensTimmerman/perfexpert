#ifndef OPTTRAN_INSTALL_DIRS_H
#define OPTTRAN_INSTALL_DIRS_H

#define OPTTRAN_PREFIX "@prefix@"
#define OPTTRAN_EXEC_PREFIX "@exec_prefix@"

/** The directory for installing executable programs that users can run. */
#define OPTTRAN_BINDIR "@bindir@"

/** The directory for object files and libraries of object code. Do not
 *  install executables here, they probably ought to go in
 *  $(libexecdir) instead.
 */
#define OPTTRAN_LIBDIR "@libdir@"

#endif /* OPTTRAN_INSTALL_DIRS_H */